{
  "name": "jay-language",
  "displayName": "Jay Language",
  "description": "Syntax highlighting for the Jay language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "jay",
      "aliases": ["Jay", "jay"],
      "extensions": [".jay"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "jay",
      "scopeName": "source.jay",
      "path": "./syntaxes/jay.tmLanguage.json"
    }]
  }
}
